<div class="popup tablet-fullscreen" id="personalDetailModal">
    <div class="view view-popup navbar-fixed">
        <div class="navbar">
            <div class="navbar-inner">
                <div class="left">
                    <a href="#" class="back link close-Form">
                        <i class="icon icon-back"></i>ข้อมูลส่วนบุคคล
                    </a>
                </div>
            </div>
        </div>
        <div class="page">
            <div class="page-content contact-edit-form">
                <div class="contact-header">
                    <div class="item-photo">
                        <img src="img/cats/{{model.picId}}.png" />
                    </div>
                    <div class="header-text">
                        <h3>{{model.firstName}} {{model.lastName}}</h3>
                        <p>{{model.company}}</p>
                    </div>
                </div>

                {{#unless data}}
                <h3>ไม่พบข้อมูลส่วนบุคคล</h3>
                {{/unless}}

                {{#if data}}
                <div class="list-block">
                    <ul>
                        <li>
                            <div class="item-content">
                                <div class="item-inner">
                                    <p>t1 : text</p>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
                {{/if}}

                <div class="list-block">
                    <ul>
                        <li>
                            <a href="#" class="item-link list-button contact-delete-button">ลบรายชื่อ</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>